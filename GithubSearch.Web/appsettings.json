{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",

  "UseInMemoryDatabase": true,
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=CleanArchitectureDb;Trusted_Connection=True;MultipleActiveResultSets=true;"
  },
  "GithubRepositories": "https://api.github.com/search/repositories",

  "IdpConfig": {
    "RedirectUri": "http://localhost:4200/assets/oidc-login-redirect.html",
    "PostLogoutRedirectUri": "http://localhost:4200/index.html?postLogout=true",
    "AllowedCorsOrigin": "http://localhost:4200"
  }
}
